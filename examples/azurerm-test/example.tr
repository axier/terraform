provider "azurerm" {
  subscription_id =
  client_id =
  client_secret =
  tenant_id =	
}

resource "azurerm_resource_group" "terraform_test" {
  name = "terraform_test"
  location = "West Europe"
}

resource "azurerm_virtual_network" "terraform_vnet" {
	name                = "terraform_vnet"
	address_space       = ["10.1.2.0/24"]
	location            = "West Europe"
	dns_servers         = ["10.1.2.10","10.1.2.11"]

	subnet {
      name           = "subnet1"
      address_prefix = "10.1.2.0/25"
	}
	resource_group_name = "terraform_test"
}

resource "azure_public_ip_address" "terraform_public_ip" {
    name = "terraform_public_ip"
    location = "west Europe"
    public_ip_allocation_method = "Dinamic"
    resource_group_name = "terraform_test"
}
